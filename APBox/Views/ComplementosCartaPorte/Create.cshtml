    //Total de impuesto Trasladado y Retenido
    $('#TTasaOCuota').on("keyup", function () {
        if ($('#TTasaOCuota').val() > 0) {

            var TBase = parseFloat($('#TBase').val());
            var TTasaOCuota = parseFloat($('#TTasaOCuota').val());
            if ($('#TBase').val() === "") {
                TBase = 0;

            }
            if ($('#TTasaOCuota').val() === "") {
                TTasaOCuota = 0;
            }

            Total = TBase * TTasaOCuota;
            $('#TTotal').val(Total);
            $('#TImporte').val(Total);
